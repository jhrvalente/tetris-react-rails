addEventListener(\"keydown\", keyPressedHandler, false);\n\n    return () => {\n      document.removeEventListener(\"keydown\", keyPressedHandler, false);\n    };\n\n  }, [keyPressedHandler]);\n\n\n  return (\n  <Router>\n    <Switch>\n      <Route path=\"/\" exact>\n        <Login/>\n      </Route>\n      <Route path=\"/dashboard\" exact>\n        <DashboardPage/>\n      </Route>\n      <Route path=\"/signup\" exact>\n        <Signup/>\n      </Route>\n      <Route path=\"/lobby\" exact>\n        <PlayersLobby/>\n      </Route>\n      <Route path=\"/highscores\" exact>\n        <HighScoresPage/>\n      </Route>\n      <Route path=\"/:userId\" exact>\n      <Home\n            //status={status} \n            //statusHandler = {startGameHandler} \n            keyControls = {keyControls} \n            //showNext={showNext} \n            //score = {score}\n            //level = {level}\n            //lines = {lines}\n            //gameOver = {gameOver} \n        /> \n      </Route>\n      <Route path=\"/:userId/games\" exact>\n      <UserGames /> \n      </Route>\n      <Route path=\"/games/multi\" exact>\n      <MultiPlayerPage\n            //status={status} \n            //statusHandler = {startGameHandler} \n            keyControls = {keyControls} \n            //showNext={showNext} \n            //score = {score}\n            //level = {level}\n            //lines = {lines}\n            //gameOver = {gameOver} \n            //enemy_status = {PLAYER2.status}\n            //enemy_gameover = {PLAYER2.gameover}\n        /> \n      </Route>\n      <Route path=\"/games/single\" exact>\n      <SinglePlayerPage\n            //status={status} \n            //statusHandler = {startGameHandler} \n            keyControls = {keyControls} \n            //showNext={showNext} \n            //score = {score}\n            //level = {level}\n            //lines = {lines}\n            //gameOver = {gameOver} \n        /> \n      </Route>\n      <Route path=\"/games/multi/:id\" exact>\n